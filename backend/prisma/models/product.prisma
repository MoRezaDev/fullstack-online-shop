model Product {
    id String @id @default(cuid())

    name String @unique
    description String

    title String
    url String
    categoryId String
    category Category @relation(fields: [categoryId],references: [id],onDelete: Cascade)

    breadcrumb Breadcrumb[]

    brand_category_url String

   
    
    seo Seo?

    colors Color[]
    
    

    orderItems OrderItem[]
}