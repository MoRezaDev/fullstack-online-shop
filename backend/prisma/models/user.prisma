model User {
  id        String    @id @default(cuid())
  mobile    String    @unique
  full_name String?
  email     String?
  password  String?
  role      String    @default("user")
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  address   Address?
  cart      Cart?
  comments  Comment[]
  orders    Order[]
  plus      Plus?
}
