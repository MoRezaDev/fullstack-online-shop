model OpenGraph {
  id        String   @id @default(cuid())
  image     String
  site      String
  title     String
  type      String
  url       String
  seoId     String   @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  seo       Seo      @relation(fields: [seoId], references: [id], onDelete: Cascade)
}
